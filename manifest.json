{
  "name": "HexTab",
  "version": "0.5.2",
  "manifest_version": 2,
  "description": "Replaces the New Tab page with lightweight Hex Colour Clock and 5-day Weather forecast.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["geolocation"],
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}
