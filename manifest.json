{
  "name": "HexTab",
  "version": "0.5.0",
  "manifest_version": 3,
  "description": "This extension was created with the awesome extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["geolocation"],
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}
